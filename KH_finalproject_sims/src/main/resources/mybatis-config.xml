		<typeAlias type="kh.finalproject.sims.biz.model.vo.BizVo" alias="bvo"/>
	</typeAliases>
	 
	<mappers>
		<mapper resource="mappers/temp-mapper.xml"/>
		<mapper resource="mappers/plan-mapper.xml"/>
		<mapper resource="mappers/biz-mapper.xml"/>
	</mappers>
</configuration>
